{
  "type": "module",
  "license": "MIT",
  "name": "sessionize_api",
  "version": "0.3.7",
  "description": "Sessionize API, with types",
  "keywords": [
    "sessionize",
    "api",
    "typescript"
  ],
  "author": "Fredrik Wigsnes",
  "homepage": "https://github.com/wigsnes/sessionize_api",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wigsnes/sessionize_api.git"
  },
  "bugs": {
    "url": "https://github.com/wigsnes/sessionize_api/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "changeset": "changeset",
    "release": "pnpm run build && changeset publish",
    "test": "vitest",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "ci": "pnpm run build && pnpm run lint && pnpm run check-exports",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "pnpm run ci"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@changesets/cli": "^2.29.7",
    "dotenv": "^17.2.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "zod": "^4.1.12"
  }
}
